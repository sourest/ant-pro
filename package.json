{
  "name": "@staryuntech/ant-pro",
  "version": "0.0.1",
  "description": "React Components Base On Ant Design",
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "typings/index.d.ts",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "prepare": "husky install",
    "type-check": "tsc --noEmit --pretty",
    "prepublishOnly": "npm run build",
    "lint": "eslint . --ext .ts,.tsx --fix --ignore-path .gitignore"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.0.0",
    "@ink-js/use-model": "^0.0.1",
    "@types/node": "^17.0.35",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "@vitejs/plugin-react": "^1.3.0",
    "antd": "^4.20.6",
    "commitlint": "^17.0.0",
    "eslint": "^8.15.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.9"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "antd": ">=4.20.0"
  },
  "directories": {
    "example": "examples"
  },
  "files": [
    "dist",
    "typings"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/staryuntech/ant-pro.git"
  },
  "keywords": [
    "React",
    "Ant Design"
  ],
  "author": "Kiro",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "bugs": {
    "url": "https://github.com/staryuntech/ant-pro/issues"
  },
  "homepage": "https://github.com/staryuntech/ant-pro#readme",
  "lint-staged": {
    "*.{json,css,less,scss,html,md}": "prettier --write",
    "*.{vue,js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
